<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rawda.prettychat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity"></activity>
        <activity android:name=".ui.view.LoginActivity" />
        <activity android:name=".ui.view.SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.view.HomeActivity" />
        <activity android:name=".ui.view.MyProfileActivity" />
        <activity android:name=".ui.view.PageActivity" />
        <activity android:name=".ui.view.MyPageActivity" />
        <activity android:name=".ui.view.AdActivity" />
        <activity android:name=".EditProfileActivity"></activity>
    </application>

</manifest>